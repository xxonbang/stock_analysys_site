# Gemini API Keys (필수)
# Google Gemini API 키를 설정하세요.
# 여러 개의 키를 설정하면 자동으로 fallback이 동작합니다.
GEMINI_API_KEY_01=your_gemini_api_key_here
GEMINI_API_KEY_02=your_second_gemini_api_key_here_optional

# Legacy: 단일 키 사용 시 (GEMINI_API_KEY_01이 없을 때만 사용)
# GEMINI_API_KEY=your_gemini_api_key_here

# Finnhub API Key (선택사항)
# Finnhub API를 사용하려면 키를 설정하세요.
# 없으면 Yahoo Finance를 사용합니다.
FINNHUB_API_KEY=your_finnhub_api_key_here

# 데이터 소스 설정 (선택사항)
# - 'auto': 자동 선택 (기본값)
# - 'finnhub': Finnhub API 우선 사용
# - 'yahoo': Yahoo Finance 사용
# - 'vercel': Python 스크립트 사용 (로컬 개발 시)
DATA_SOURCE=auto

# Python 스크립트 사용 여부 (선택사항)
# 로컬 개발 환경에서 Python 스크립트를 직접 실행하려면 true로 설정
# 기본값: false (Vercel 환경에서는 자동으로 감지)
USE_PYTHON_SCRIPT=false

# Python 경로 (선택사항)
# Python 3.10 이상의 경로를 명시적으로 지정하려면 설정
# 예: PYTHON_PATH=/usr/local/bin/python3.11
# 예: PYTHON_PATH=python3.11
# 설정하지 않으면 자동으로 python3.11, python3.10, python3, python 순으로 찾습니다.
# PYTHON_PATH=python3.11

# 동적 티커 매핑 사용 여부 (선택사항)
# 한국 주식 이름을 티커로 변환할 때 동적 검색을 사용할지 여부
# 기본값: true
USE_DYNAMIC_TICKER_MAPPING=true

# 앱 URL (선택사항)
NEXT_PUBLIC_APP_URL=http://localhost:3000

# 로그 레벨 (선택사항)
# - 'debug': 모든 로그 출력
# - 'info': 정보 이상 로그 출력
# - 'warn': 경고 이상 로그 출력
# - 'error': 에러만 출력
# 기본값: 개발 환경에서는 'debug', 프로덕션에서는 'error'
LOG_LEVEL=debug
